<div id="auth_form" class="clear">
   <p id="pagetitle">
    <h1><%= title %></h1>
  </p>
  <div id="contents" class="clear">
  <form id="login" action="/lib" method="post" class="box_effect">
      <%- message('passErr') %>
      <div class="inputs">
        <p><label for="change_username">Username</label></p>
        <p><input id="change_username" type="text" name="username"
                  size="30" placeholder="username"></p>
        <p><label for="change_password">Old password</label></p>
        <p><input id="change_password" type="password" name="oldPassword"
                  size="30" placeholder="old password"></p>
        <p><input type="submit" class="newpost" value="Submit"></p>
      </div>
  </form>
</div>
</div>
